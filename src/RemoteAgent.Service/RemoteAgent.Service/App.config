<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="processConfiguration" type="RemoteAgent.Service.Configuration.ProcessConfiguration, RemoteAgent.Service"/>
	</configSections>
	<processConfiguration>
		<launcherItems>
			<add name="notepad" path="notepad.exe"/>
		</launcherItems>
		<closableProcessesSettings namePattern="notepad" />
	</processConfiguration>
	<appSettings>
		<add key="serviceName" value="RemoteAgent.Service Name"/>
		<add key="serviceDisplayName" value="RemoteAgent.Service DisplayName"/>
		<add key="serviceDescription" value="RemoteAgent.Service Description"/>

		<add key="EncryptionPhrase" value="EncryptionPhrase"/>
		<add key="TcpListenerPort" value="60002"/>
		<add key="TcpBackLogSize" value="120"/>
		<add key="UdpBeaconPort" value="60001"/>
		<add key="UdpBeaconPollingInterval" value="1000"/>
		<add key="AgentName" value="PC Service agent"/>
	</appSettings>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
	</startup>
</configuration>